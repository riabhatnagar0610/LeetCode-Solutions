class Solution {
    public boolean isPalindrome(int x) {
        int num; 
        int rev = 0;
        num = x;
        while(x>0){
            rev = rev * 10 + x%10;
            x/=10;
        }

        if(num==rev){
            return true;
        }
        else{
            return false;
        }
    }
}
