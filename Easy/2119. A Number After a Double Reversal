class Solution {
    public boolean isSameAfterReversals(int num) {
        int temp = num;
        int rev1 = 0;
        int rev2 = 0;
        while(temp != 0){
            rev1 = rev1*10 + temp%10;
            temp /= 10;
        }
        temp = rev1;
        while(temp != 0){
            rev2 = rev2*10 + temp%10;
            temp /= 10;
        }

        return rev2==num;
    }
}
